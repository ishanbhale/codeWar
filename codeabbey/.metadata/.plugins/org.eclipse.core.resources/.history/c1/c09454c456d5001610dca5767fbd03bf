package com.program.sol8;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Solution {

	public static void main(String[] args) throws FileNotFoundException {
		File file = new File("D:/Study/Prog_Input/myfile.txt");
		Scanner in = new Scanner(file);
		long computedValue = in.nextLong();
		char op = '%';
		long value = 0 ;
		while (in.hasNext()) {
			op = in.next().charAt(0);
			switch (op) {
			
			case '+':
				value = in.nextLong();
				computedValue = computedValue + value;
				break;
			case '*':
				value = in.nextLong();
				computedValue = computedValue * value;
				break;
			case '%':
				value = in.nextLong();
				computedValue = computedValue % value;
				break;
			}
			
			
			
			
		}
		switch (op){
	case '+':
		computedValue = computedValue + in.nextLong();
		break;
	case '*':
		computedValue = computedValue * in.nextLong();
		break;
	case '%':
		computedValue = computedValue % in.nextLong();
		break;
	}
		System.out.println(computedValue);
	}
}
