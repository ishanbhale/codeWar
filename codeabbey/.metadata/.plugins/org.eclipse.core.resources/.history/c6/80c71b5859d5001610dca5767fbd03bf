package com.program.sol8;

import java.io.File;
import java.io.FileNotFoundException;
import java.math.BigInteger;
import java.util.Scanner;

public class Solution {

	public static void main(String[] args) throws FileNotFoundException {
		File file = new File("D:/Study/Prog_Input/myfile.txt");
		Scanner in = new Scanner(file);
		BigInteger computedValue=new BigInteger(in.next());  
		char op = '%';
		BigInteger value = new BigInteger(in.next());
		while (in.hasNext()) {
			op = in.next().charAt(0);
			switch (op) {
			
			case '+':
				value = in.next();
				computedValue = computedValue.add(value);
				break;
			case '*':
				value = in.nextLong();
				computedValue = computedValue * value;
				break;
			case '%':
				value = in.nextLong();
				computedValue = computedValue % value;
				break;
			}
			
			
			
			
		}
		switch (op){
	case '+':
		computedValue = (computedValue - value) % value;
		
		break;
	case '*':
		computedValue = (computedValue / value)% value;
		break;
	case '%':
		
		break;
	}
		System.out.println(computedValue);
	}
}
