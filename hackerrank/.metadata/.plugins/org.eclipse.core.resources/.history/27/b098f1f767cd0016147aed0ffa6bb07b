import java.text.*;
import java.math.*;
import java.util.regex.*;
class Deal implements Comparable{
    int tCnt;
    int bribe;
    public int compareTo(Object b){
        Deal d=(Deal)b;
        return this.bribe-d.bribe;
    }
}
