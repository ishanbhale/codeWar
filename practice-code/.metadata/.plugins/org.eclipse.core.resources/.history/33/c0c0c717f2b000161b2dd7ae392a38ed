package com.implement.list;

public class ArrayListImpl {
	Object[] elementData; 
	int size = 0;
	void add(Object o){
		if(elementData.length - size < 0){
			size = (size * 3)/2 + 1;
		}
		
		
	}
	Object[] get(){
		return elementData;
	}
	public static void main(String[] args) {
		
	ArrayListImpl obj = new ArrayListImpl();
		
		for (Object a: obj.get())
		{
			System.out.println(a);
		}
	}

}
