package com.implement.treeandgraphs;

class Node {
	Node left;
	Node right;
	int data;
	public Node(int data)
    {
        this.data = data;
    }
}



public class BinaryTreeImpl {

Node parent;
int data;
int size;

	public BinaryTreeImpl() {
	super();
	parent = new Node(data);
}
	
	void add(int data){
		if(size == 0){
			parent.data = data;
			size++;
		}
	}

	public static void main(String[] args) {
	BinaryTreeImpl obj = new BinaryTreeImpl();
	obj.add(30);

	}

}
